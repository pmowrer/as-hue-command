parser: babel-eslint
rules:
  indent:
    - 2
    - 2
  quotes:
    - 2
    - single
    - avoid-escape
  linebreak-style:
    - 2
    - unix
  semi:
    - 2
    - always
  no-console:
    - 2
    - allow:
      - warn
      - error
env:
  es6: true
  browser: true
extends: 'eslint:recommended'
ecmaVersion: 6
ecmaFeatures:
  experimentalObjectRestSpread: true
